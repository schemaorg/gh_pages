            <div class="examples-section">
                <h2>Examples</h2>
                <div class="example-tabs">
                    <div class="example-tab active" onclick="showExample('jsonld')">JSON-LD</div>
                    <div class="example-tab" onclick="showExample('microdata')">Microdata</div>
                    <div class="example-tab" onclick="showExample('rdfa')">RDFa</div>
                </div>
                
                <div id="jsonld-example" class="example-content">
                    <h3>JSON-LD</h3>
                    <pre><code>{
  "@context": "https://schema.org",
  "@type": "Person",
  "name": "Jane Doe",
  "image": "janedoe.jpg",
  "jobTitle": "Professor",
  "telephone": "(425) 123-4567",
  "email": "jane-doe@example.com",
  "url": "http://www.janedoe.com",
  "address": {
    "@type": "PostalAddress",
    "addressLocality": "Seattle",
    "addressRegion": "WA",
    "postalCode": "98052",
    "streetAddress": "20341 Whitworth Institute 405 N. Whitworth"
  },
  "colleague": [
    "http://www.example.com/colleagues/john-doe",
    "http://www.example.com/colleagues/jane-smith"
  ]
}</code></pre>
                </div>
                
                <div id="microdata-example" class="example-content" style="display:none;">
                    <h3>Microdata</h3>
                    <pre><code>&lt;div itemscope itemtype="https://schema.org/Person"&gt;
  &lt;img src="janedoe.jpg" itemprop="image" alt="Photo of Jane Doe"/&gt;
  &lt;h1 itemprop="name"&gt;Jane Doe&lt;/h1&gt;
  &lt;span itemprop="jobTitle"&gt;Professor&lt;/span&gt;
  &lt;div itemprop="address" itemscope itemtype="https://schema.org/PostalAddress"&gt;
    &lt;span itemprop="streetAddress"&gt;20341 Whitworth Institute 405 N. Whitworth&lt;/span&gt;
    &lt;span itemprop="addressLocality"&gt;Seattle&lt;/span&gt;,
    &lt;span itemprop="addressRegion"&gt;WA&lt;/span&gt;
    &lt;span itemprop="postalCode"&gt;98052&lt;/span&gt;
  &lt;/div&gt;
  &lt;span itemprop="telephone"&gt;(425) 123-4567&lt;/span&gt;
  &lt;a href="mailto:jane-doe@example.com" itemprop="email"&gt;jane-doe@example.com&lt;/a&gt;
  &lt;a href="http://www.janedoe.com" itemprop="url"&gt;janedoe.com&lt;/a&gt;
  Colleagues:
  &lt;a href="http://www.example.com/colleagues/john-doe" itemprop="colleague"&gt;John Doe&lt;/a&gt;
  &lt;a href="http://www.example.com/colleagues/jane-smith" itemprop="colleague"&gt;Jane Smith&lt;/a&gt;
&lt;/div&gt;</code></pre>
                </div>
                
                <div id="rdfa-example" class="example-content" style="display:none;">
                    <h3>RDFa</h3>
                    <pre><code>&lt;div vocab="https://schema.org/" typeof="Person"&gt;
  &lt;img src="janedoe.jpg" property="image" alt="Photo of Jane Doe"/&gt;
  &lt;h1 property="name"&gt;Jane Doe&lt;/h1&gt;
  &lt;span property="jobTitle"&gt;Professor&lt;/span&gt;
  &lt;div property="address" typeof="PostalAddress"&gt;
    &lt;span property="streetAddress"&gt;20341 Whitworth Institute 405 N. Whitworth&lt;/span&gt;
    &lt;span property="addressLocality"&gt;Seattle&lt;/span&gt;,
    &lt;span property="addressRegion"&gt;WA&lt;/span&gt;
    &lt;span property="postalCode"&gt;98052&lt;/span&gt;
  &lt;/div&gt;
  &lt;span property="telephone"&gt;(425) 123-4567&lt;/span&gt;
  &lt;a href="mailto:jane-doe@example.com" property="email"&gt;jane-doe@example.com&lt;/a&gt;
  &lt;a href="http://www.janedoe.com" property="url"&gt;janedoe.com&lt;/a&gt;
  Colleagues:
  &lt;a href="http://www.example.com/colleagues/john-doe" property="colleague"&gt;John Doe&lt;/a&gt;
  &lt;a href="http://www.example.com/colleagues/jane-smith" property="colleague"&gt;Jane Smith&lt;/a&gt;
&lt;/div&gt;</code></pre>
                </div>
            </div>
        </div>
    </div>
    
    <script>
        function showExample(format) {
            // Hide all examples
            document.getElementById('jsonld-example').style.display = 'none';
            document.getElementById('microdata-example').style.display = 'none';
            document.getElementById('rdfa-example').style.display = 'none';
            
            // Remove active class from all tabs
            var tabs = document.querySelectorAll('.example-tab');
            tabs.forEach(function(tab) {
                tab.classList.remove('active');
            });
            
            // Show selected example and activate tab
            document.getElementById(format + '-example').style.display = 'block';
            event.target.classList.add('active');
        }
    </script>
</body>
